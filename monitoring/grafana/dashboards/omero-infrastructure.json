  "panels": [
    {
      "type": "table",
      "title": "Server information",
      "description": "Single-host specification list sourced only from node-exporter metrics.",
      "id": 16,
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "left",
            "cellOptions": {
              "type": "auto"
            }
          },
          "unit": "none",
          "decimals": 2
        },
        "overrides": []
      },
      "options": {
        "showHeader": false,
        "footer": {
          "show": false
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "max by (spec, value) (label_replace(label_replace(node_os_info{job=~\"node-exporter|node_exporter\"}, \"spec\", \"Operating system\", \"__name__\", \".*\"), \"value\", \"$1\", \"pretty_name\", \"(.*)\")) or max by (spec, value) (label_replace(label_replace(node_uname_info{job=~\"node-exporter|node_exporter\"}, \"spec\", \"Kernel version\", \"__name__\", \".*\"), \"value\", \"$1\", \"release\", \"(.*)\")) or max by (spec, value) (label_replace(label_replace(node_uname_info{job=~\"node-exporter|node_exporter\"}, \"spec\", \"Architecture\", \"__name__\", \".*\"), \"value\", \"$1\", \"machine\", \"(.*)\")) or max by (spec, value) (label_replace(count_values(\"value\", count by (instance) (node_cpu_seconds_total{job=~\"node-exporter|node_exporter\", mode=\"idle\"})), \"spec\", \"CPU cores\", \"__name__\", \".*\")) or max by (spec, value) (label_replace(count_values(\"value\", (node_memory_MemTotal_bytes{job=~\"node-exporter|node_exporter\"} / 1000^3)), \"spec\", \"Memory total (GB)\", \"__name__\", \".*\")) or max by (spec, value) (label_replace(count_values(\"value\", (node_memory_SwapTotal_bytes{job=~\"node-exporter|node_exporter\"} / 1000^3)), \"spec\", \"Swap total (GB)\", \"__name__\", \".*\")) or max by (spec, value) (label_replace(count_values(\"value\", ((time() - node_boot_time_seconds{job=~\"node-exporter|node_exporter\"}) / 3600)), \"spec\", \"Uptime (hours)\", \"__name__\", \".*\"))",
          "format": "table",
          "instant": true
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "Value": true,
              "__name__": true,
              "instance": true,
              "job": true,
              "domainname": true,
              "machine": true,
              "nodename": true,
              "release": true,
              "sysname": true,
              "version": true,
              "pretty_name": true
            },
            "renameByName": {
              "spec": "Information",
              "value": "Value"
            },
            "indexByName": {
              "spec": 0,
              "value": 1
            }
          }
        }
      ]
    },
    {
      "type": "stat",
      "title": "Host CPU usage",
      "id": 1,
      "gridPos": {
        "h": 3,
        "w": 4,
        "x": 8,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 100,
          "decimals": 1,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 0.75
              }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "(1 - avg(rate(node_cpu_seconds_total{job=~\"node-exporter|node_exporter\", mode=\"idle\"}[5m])))"
        }
      ],
      "options": {
        "graphMode": "none"
      }
    },
