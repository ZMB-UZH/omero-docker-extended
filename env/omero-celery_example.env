OMERO_JOB_SERVICE_USERNAME=job-service
# OMERO_JOB_SERVICE_PASS is defined in env/omero_secrets.env
OMERO_JOB_SERVICE_GROUP=
OMERO_JOB_SERVICE_SECURE=true
OMERO_WEB_JOB_SERVICE_USERNAME=job-service
# OMERO_WEB_JOB_SERVICE_PASS needs to match OMERO_JOB_SERVICE_PASS
# OMERO_WEB_JOB_SERVICE_PASS is defined in env/omero_secrets.env
#
OMERO_IMS_USE_CELERY=true
OMERO_IMS_CELERY_BROKER_URL=redis://redis:6379/2
OMERO_IMS_CELERY_BACKEND_URL=redis://redis:6379/2
OMERO_IMS_CELERY_QUEUE=imaris_export
OMERO_IMS_CELERY_RESULT_EXPIRES=7200
OMERO_IMS_CELERY_TIME_LIMIT=7200
OMERO_IMS_CELERY_LOGLEVEL=info
OMERO_IMS_CELERY_WORKER_CONCURRENCY=1
OMERO_IMS_CELERY_MAX_RETRIES=20
OMERO_IMS_CELERY_PREFETCH=1
OMERO_IMS_SCRIPT_NAME=IMS_Export.py
OMERO_IMS_EXPORT_DIR=/OMERO/ImarisExports
OMERO_IMS_EXPORT_TIMEOUT=3600
OMERO_IMS_EXPORT_POLL_INTERVAL=2.0
OMERO_IMS_SCRIPT_START_TIMEOUT=180
OMERO_IMS_SCRIPT_START_RETRY_INTERVAL=5
OMERO_IMS_PROCESS_JOB_DIR=/tmp/omero_ims_process_jobs
OMERO_IMS_PROCESSOR_CONFIG_CACHE_TTL=30
OMERO_IMS_USE_JOB_SERVICE_SESSION=true
