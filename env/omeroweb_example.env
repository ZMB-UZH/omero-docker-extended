OMEROHOST=omeroserver
OMERO_PORT=4064
OMERO_WEB_ROOT=/opt/omero/web
CONFIG_omero_security_transport=ssl
CONFIG_omero_security_ssl=true
# OMP_HASH_SECRET is defined in env/omero_secrets.env
# FMP_HASH_SECRET is defined in env/omero_secrets.env
OMP_DATA_USER=omero-plugin
# OMP_DATA_PASS is defined in env/omero_secrets.env
OMP_DATA_HOST=database_plugin
OMP_DATA_DB=omero-plugin
OMP_DATA_PORT=5433
CONFIG_omero_web_application__server_port=4090
CONFIG_omero_web_session__cookie__secure=false
CONFIG_omero_web_time__zone=Europe/Zurich
TZ=Europe/Zurich
#
# This MUST match the volume mount in docker-compose.yml.
# The log directory is mounted at /opt/omero/web/OMERO.web/var/log
# and must be writable BEFORE zarr imports during OMERO.web startup.
# ------------------------------------------------------------------
CONFIG_omero_web_logdir=/opt/omero/web/OMERO.web/var/log
#
# Change the URL based on the actual domain of the reverse proxy. Comment out if no reverse proxy is in use.
# ----------------------------------------------------------------------------------------------------------
#CONFIG_omero_web_csrf__trusted__origins='["https://mydomain.com"]'
#
CONFIG_omero_web_caches={"default":{"BACKEND":"django.core.cache.backends.redis.RedisCache","LOCATION":"redis://redis:6379/1","KEY_PREFIX":"omero","TIMEOUT":300}}
CONFIG_omero_web_apps=["omero_gallery","omero_figure","omero_fpbioimage","omero_iviewer","omero_mapr","omero_parade","omero_web_zarr","omero_autotag","omero_tagsearch","omeroweb_omp_plugin","omeroweb_upload","omeroweb_admin_tools","omeroweb_imaris_connector"]
CONFIG_omero_web_ui_center__plugins=[["Parade","omero_parade/init.js.html","omero_parade"]]
CONFIG_omero_web_open__with=[["Image viewer","webgateway",{"supported_objects":["image"],"script_url":"webclient/javascript/ome.openwith_viewer.js"}],["omero_figure","new_figure",{"supported_objects":["images"],"target":"_blank","label":"OMERO.Figure"}],["omero_iviewer","omero_iviewer_index",{"supported_objects":["images","dataset","well"],"script_url":"omero_iviewer/openwith.js","label":"OMERO viewer"}],["omero_fpbioimage","fpbioimage_index",{"supported_objects":["image"],"script_url":"fpbioimage/openwith.js","label":"FPBioimage"}],["web_zarr_vizarr","omero_web_zarr_index",{"supported_objects":["image"],"label":"Vizarr","script_url":"omero_web_zarr/openwith.js"}]]
CONFIG_omero_web_ui_top__links=[["Data","webindex",{"title":"Browse Data via Projects, Tags etc"}],["Figure","figure_index",{"title":"Open Figure in new tab","target":"_blank"}],["Mapr",{"viewname":"maprindex_anyvalue"},{"title":"Find Any Value"}],["Gallery","/gallery/",{"title":"Open OMERO Gallery","target":"_blank"}],["Tag Search","tagsearch",{"title":"Tag Search"}],["History","history",{"title":"History"}],["Help","https://help.openmicroscopy.org/",{"title":"Open OMERO user guide in a new tab","target":"new"}],["OMP plugin","omeroweb_omp_plugin_index",{"title":"Open OMP plugin in new tab","target":"_blank"}],["Upload","omeroweb_upload_index",{"title":"Open Upload in new tab","target":"_blank"}],["Admin tools","omeroweb_admin_tools_index",{"title":"Open Admin tools in new tab","target":"_blank"}]]
CONFIG_omero_web_login__logo=/static/branding/logo.png
#
# Storage quota enforcement
# -------------------------
ADMIN_TOOLS_QUOTA_RECONCILE_INTERVAL_SECONDS=60
# Minimum allowed quota value in GB (UI validation + backend validation + ext4 enforcer)
ADMIN_TOOLS_MIN_QUOTA_GB=0.10
# Default quota value in GB automatically assigned to new OMERO groups
ADMIN_TOOLS_DEFAULT_GROUP_QUOTA_GB=0.10
# Set true to auto-create per-group quota entries for new OMERO groups
ADMIN_TOOLS_AUTO_SET_DEFAULT_GROUP_QUOTA=false
#
# Quota state file â€“ MUST be on the shared /OMERO volume so the host-side
# systemd enforcer can read it.  Default: /OMERO/.admin-tools/group-quotas.json
# ADMIN_TOOLS_QUOTA_STATE_PATH=/OMERO/.admin-tools/group-quotas.json
#
# ext4 project-quota mapping files (used by the host-side enforcer only)
# ----------------------------------------------------------------------
ADMIN_TOOLS_QUOTA_PROJECTS_FILE=/OMERO/.admin-tools/quota/projects
ADMIN_TOOLS_QUOTA_PROJID_FILE=/OMERO/.admin-tools/quota/projid
ADMIN_TOOLS_QUOTA_PROJECT_ID_MIN=200000
#
ADMIN_TOOLS_LOKI_URL=http://loki:3100
ADMIN_TOOLS_LOG_LOOKBACK_SECONDS=900
ADMIN_TOOLS_LOG_MAX_ENTRIES=5000
ADMIN_TOOLS_LOG_REQUEST_TIMEOUT_SECONDS=10
#
ADMIN_TOOLS_GRAFANA_URL=http://grafana:3000
ADMIN_TOOLS_GRAFANA_DASHBOARD_UID=omero-infrastructure
ADMIN_TOOLS_GRAFANA_DASHBOARD_SLUG=server-infrastructure
ADMIN_TOOLS_PROMETHEUS_URL=http://prometheus:9090
#
OMERO_WEB_UPLOAD_CONCURRENCY=3
OMERO_WEB_UPLOAD_BATCH_FILES=5
OMERO_WEB_UPLOAD_DISABLE_SPECIAL_METHODS=false
OMERO_WEB_UPLOAD_DIR=/tmp/omero-upload-tmp
OMERO_WEB_UPLOAD_JOBS_DIR=/tmp/omero_web_upload_jobs
OMERO_WEB_UPLOAD_CLEANUP_INTERVAL=300
OMERO_WEB_UPLOAD_CLEANUP_MAX_AGE=43200
OMERO_WEB_UPLOAD_CLEANUP_STALE_AGE=172800
OMERO_WEB_UPLOAD_CLEANUP_MAX_DELETE=25
#
CONFIG_omero_web_debug=true
OMERO_WEB_VENV=venv-3.12
#
